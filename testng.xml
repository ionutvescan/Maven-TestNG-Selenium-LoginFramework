<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests">
    <listeners>
        <listener class-name="Utilities.Listeners"></listener>
    </listeners>
    <test verbose="2" preserve-order="true" name="End-To-End Tests">
        <classes>
            <class name="LoginTestCases.EndToEndTests"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Successful Login Tests">
        <classes>
            <class name="LoginTestCases.SuccessfulLoginTests"/>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Unsuccessful Login Tests">
            <classes>
                <class name="LoginTestCases.UnsuccessfulLoginTests"/>
        </classes>
    </test>
</suite>